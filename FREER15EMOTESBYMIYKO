local WindUI

do
    local ok, result = pcall(function()
        return require("./src/Init")
    end)
    
    if ok then
        WindUI = result
    else 
        WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()
    end
end

if not WindUI then
    warn("Mi mamada ya no sirve")
else
    print("Gud")
end

local Window = WindUI:CreateWindow({
    Title = "Exploiters Basement Hub (Emotes R15)",
    Author = "by MIYKO",
    Folder = "MIYKOthings",
    NewElements = true,
    HideSearchBar = false,
    Background = "rbxassetid://74876370358120",
    OpenButton = {
        Title = "OPEN",
        CornerRadius = UDim.new(1,0),
        StrokeThickness = 3,
        Enabled = true,
        Draggable = true,
        OnlyMobile = false,
        Color = ColorSequence.new(
            Color3.fromHex("#30FF6A"), 
            Color3.fromHex("#e7ff2f")
        )
    }
})

local function NotifyAll()
    WindUI:Notify({
        Title = "IMPORTANT",
        Content = "Emotes By MIYKO",
        Icon = "info"
    })
end

local MainSection = Window:Section({ Title = "Settings" })



-- BEST EMOTOS
local Tab = MainSection:Tab({
    Title = "Best Emotes R15",
    Icon = "user",
})






Tab:Button({
    Title = "Hakari Dance",
    Desc = "By-MIYKO",
    Callback = function()
        local animationId = 86208454108756
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://" .. animationId
local track = humanoid:LoadAnimation(anim)
track:Play()
        NotifyAll()
    end
})

