local WindUI

do
    local ok, result = pcall(function()
        return require("./src/Init")
    end)
    
    if ok then
        WindUI = result
    else 
        WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()
    end
end

if not WindUI then
    warn("Mi mamada ya no sirve")
else
    print("Gud")
end

local Window = WindUI:CreateWindow({
    Title = "Exploiters Basement Hub (Emotes R15)",
    Author = "by MIYKO",
    Folder = "MIYKOthings",
    NewElements = true,
    HideSearchBar = false,
    Background = "rbxassetid://74876370358120",
    OpenButton = {
        Title = "OPEN",
        CornerRadius = UDim.new(1,0),
        StrokeThickness = 3,
        Enabled = true,
        Draggable = true,
        OnlyMobile = false,
        Color = ColorSequence.new(
            Color3.fromHex("#30FF6A"), 
            Color3.fromHex("#e7ff2f")
        )
    }
})

local function NotifyAll()
    WindUI:Notify({
        Title = "IMPORTANTE",
        Content = "Emotes Por MIYKO, Minhnat Es un pendejo",
        Icon = "info"
    })
end

local MainSection = Window:Section({ Title = "Settings" })

local Tab1 = MainSection:Tab({
    Title = "Emotes R15",
    Icon = "user",
})

Tab1:Button({
    Title = "Confess Dance V2",
    Desc = "By-MIYKO",
    Callback = function()
        local animationId = 109585388574936
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://" .. animationId
local track = humanoid:LoadAnimation(anim)
track:Play()
        NotifyAll()
    end
})

