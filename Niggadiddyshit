local UI1 = loadstring(game:HttpGet("https://raw.githubusercontent.com/x2zu/OPEN-SOURCE-UI-ROBLOX/refs/heads/main/X2ZU%20UI%20ROBLOX%20OPEN%20SOURCE/DummyUi-leak-by-x2zu/fetching-main/Tools/Framework.luau"))()

local Win1 = UI1:Window({
    Title = "Exploiters Paradise",
    Desc = "TSB Free Emotes",
    Icon = 96482086458333,
    Theme = "Dark",
    Config = {
        Keybind = Enum.KeyCode.LeftControl,
        Size = UDim2.new(0, 500, 0, 400)
    },
    CloseUIButton = {
        Enabled = true,
        Text = "Emotes"
    }
})

local function NotifyAll()
    Win1:Notify({
    Title = "Emotes By",
    Desc = "MIYKO & Lilith",
    Time = 4
})
end

local Tab1 = Win1:Tab({Title = "Emotes", Icon = "user"})

Tab1:Section({Title = "Emotes"})

Tab1:Button({
    Title = "Anything To Look Cool",
    Desc = "Free Emote",
    Callback = function()
loadstring(game:HttpGet("https://arch-http.vercel.app/files/Anything To Look Cool"))()
NotifyAll()
    end
})

Tab1:Button({
    Title = "Take The L",
    Desc = "Free Emote",
    Callback = function()
local p = game.Players.LocalPlayer
local c, h = p.Character, p.Character:WaitForChild("Humanoid")
local a = Instance.new("Animation")
a.AnimationId = "rbxassetid://18614546390"
h:LoadAnimation(a):Play()
NotifyAll()
    end
})

